   <text:p text:style-name="P59"><text:span text:style-name="T5">Dexcription:</text:span> List current program text.</text:p>
   <text:p text:style-name="P15">Syntax:</text:p>
   <text:p text:style-name="P18">LIST [startLine[-endLine]]</text:p>
