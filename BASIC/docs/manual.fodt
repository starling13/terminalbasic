   <text:h text:style-name="Heading_20_4" text:outline-level="4"><text:bookmark-start text:name="__RefHeading___Toc549_1089627072"/>POKE<text:bookmark-end text:name="__RefHeading___Toc549_1089627072"/></text:h>
   <text:h text:style-name="Heading_20_4" text:outline-level="4"><text:bookmark-start text:name="__RefHeading___Toc1253_1116085189"/>PRINT<text:bookmark-end text:name="__RefHeading___Toc1253_1116085189"/></text:h>
   <text:p text:style-name="P34"><text:span text:style-name="T9">Description</text:span><text:bookmark-start text:name="__DdeLink__1458_145518910"/><text:span text:style-name="T5">:</text:span> <text:bookmark-end text:name="__DdeLink__1458_145518910"/><text:span text:style-name="T39">Output the expressions values to the standart output.</text:span></text:p>
   <text:p text:style-name="P6"><text:span text:style-name="T39">Syntax</text:span>:</text:p>
   <text:p text:style-name="P5">PRINT <text:span text:style-name="T19">[ [ expression</text:span> <text:span text:style-name="T20">| ; | , </text:span>]*</text:p>
   <text:p text:style-name="P7"><text:span text:style-name="T39">Examples</text:span>:</text:p>
   <text:p text:style-name="P78">PRINT «<text:span text:style-name="T15">Hello</text:span>, <text:span text:style-name="T15">World</text:span>!»</text:p>
   <text:p text:style-name="P91">Hello World</text:p>
   <text:p text:style-name="P34"/>
   <text:p text:style-name="P75"><text:soft-page-break/>PRINT «<text:span text:style-name="T21">First line</text:span>»</text:p>
