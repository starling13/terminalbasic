   <text:p text:style-name="P89">First line</text:p>
   <text:p text:style-name="P91"/>
   <text:p text:style-name="P89">Second one</text:p>
   <text:p text:style-name="P34"/>
   <text:p text:style-name="P71">PRINT 2*2</text:p>
   <text:p text:style-name="P91">4</text:p>
   <text:p text:style-name="P34"/>
   <text:p text:style-name="P71">PRINT «47.31+24.5^2=», 47.31+24.5^2</text:p>
   <text:p text:style-name="P91"><text:span text:style-name="T16">47.31+24.5^2=<text:tab/><text:tab/></text:span>647.56</text:p>
