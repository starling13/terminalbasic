          <mrow>
           <msup>
            <mi>x</mi>
            <mn>2</mn>
           </msup>
           <mo stretchy="false">+</mo>
           <msup>
            <mi>y</mi>
            <mn>2</mn>
           </msup>
          </mrow>
         </msqrt>
        </mrow>
        <annotation encoding="StarMath 5.0">z = sqrt { x sup 2 + y sup 2 }</annotation>
       </semantics>
      </math>
     </draw:object><draw:image>
