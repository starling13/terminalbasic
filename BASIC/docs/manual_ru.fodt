   <text:p text:style-name="P163"><text:span text:style-name="T25">Синтаксис</text:span>:</text:p>
   <text:p text:style-name="P162"><text:span text:style-name="T43">FWRITE</text:span> <text:span text:style-name="T43">&lt;целочисленное выражение&gt;, </text:span>&lt;<text:span text:style-name="T1">дескриптор файла</text:span>&gt;</text:p>
